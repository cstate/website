/**
 * Links
 */
a {
  color: #FF4D00;
  text-decoration: none; }
  a:hover {
    color: #FFC100; }

.markdown a {
  text-decoration: none;
  border-bottom: 1px solid currentColor; }

.icon {
  height: 40px;
  width: auto; }

hr {
  display: block;
